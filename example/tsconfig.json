{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "strict": true,
    "strictPropertyInitialization": false,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitThis": true,
    "allowJs": true,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom"
    ],
    "outDir": "lib",
    "types": [
      "node"
    ],
  },
  "include": [
    "test/*.ts"
  ]
}